<%= link_to 'Lets Play!', new_game_path %>

<br>

<h3>Available Games List</h3>

<% @games.each do |game| %>
  <h1><%= game.name %></h1>
<% end %>

<h3>My Games List</h3>

<% if current_user.present? %>
  <% @my_games.each do |game| %>
    <h1><%= game.name %></h1>
  <% end %>
<% end %>
